{
  "rewrites": [
    {
      "source": "/",
      "destination": "/react-app.html"
    },
    {
      "source": "/react-app",
      "destination": "/react-app.html"
    },
    {
      "source": "/old-index",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)\\.(js|css|json|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot)",
      "destination": "/$1.$2"
    },
    {
      "source": "/(.*)",
      "destination": "/react-app.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin-allow-popups"
        }
      ]
    }
  ]
} 